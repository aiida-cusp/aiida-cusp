{
    "name": "aiida-cusp",
    "version": "0.0.0",
    "author": "Andreas Stamminger",
    "author_email": "stammingera@gmail.com",
    "classifiers": [
			"Development Status :: 1 - Planning",
    	"Programming Language :: Python :: 3",
			"Topic :: Scientific/Engineering :: Physics",
			"License :: OSI Approved :: MIT License",
			"Environment :: Plugins",
			"Framework :: AiiDA"
		],
    "entry_points": {
			"aiida.data": [
				"cusp.kpoints = aiida_cusp.data.inputs.vasp_kpoint:VaspKpointData",
				"cusp.potcar_file_node = aiida_cusp.data.inputs.vasp_potcar:VaspPotcarFile"
			]
    },
    "install_requires": [
      "aiida-core>=1.0.0a1,<2.0.0",
      "custodian",
      "pymatgen"
    ],
    "extras_require": {
      "docs": [
        "sphinx",
        "sphinx_rtd_theme"
      ],
      "develop": [
				"pre-commit",
				"pytest",
				"pgtest"
      ]
    },
    "license": "MIT License",
    "url": "https://github.com/astamminger/aiida-cusp"
}
